    <link rel="stylesheet" href="/stylesheets/basic.css">
    <style>
        #line {
            width: 265px;
            height: 2px;
            background-color: #a86060;
            margin-left: 12px;
        }

        .inner {
            overflow: hidden;
        }

        .inner img {
            transition: all 1.5s ease;
        }

        .inner:hover img {
            transform: scale(1.5);
        }

        .fa-star {
            color: #f8c704;
        }
    </style>

